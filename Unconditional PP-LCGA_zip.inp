TITLE: 
Unconditional PP-LCGA;
!1-6 Classes;
!ZIP;

DATA:
FILE IS 
"/Input_Mplus.dat";

VARIABLE: 
NAMES = ID itAlc1 itAlc2 itAlc3 itAlc4 EXT1 EXT2 EXT3 EXT4 WithDep1 
WithDep2 WithDep3 WithDep4 FamilyCluster SiteStrata; 

USEVARIABLES = EXT1 EXT2 EXT3 EXT4 WithDep1 WithDep2 WithDep3 
               WithDep4 itAlc1 itAlc2 itAlc3 itAlc4;
COUNT = itAlc1 - itAlc4 (i);
MISSING ARE ALL (-999);         
IDVARIABLE = ID;
STRATIFICATION = SiteStrata;
CLUSTER = FamilyCluster;
CLASSES= c(4);

DEFINE: FamilyCluster= FamilyCluster+1000000*SiteStrata;

ANALYSIS:
    ALGORITHM = INTEGRATION;
    ESTIMATOR IS MLR;
    TYPE = COMPLEX MIXTURE;
    STARTS = 1000 250;
    STITERATIONS = 20;
    PROCESSOR = 4;

MODEL:
   %OVERALL%
      i1 s1 q1 | EXT1@0 EXT2@1 EXT3@2 EXT4@3;
          i1-q1@0;
      i2 s2 q2 | itAlc1@0 itAlc2@1 itAlc3@2 itAlc4@3;
      ii2 si2 qi2 | itAlc1#1@0 itAlc2#1@1 itAlc3#1@2 itAlc4#1@3;
          s2-qi2@0;
      i3 s3 q3 | WithDep1@0 WithDep2@1 WithDep3@2 WithDep4@3;
          i3-q3@0;
        !LCGA: Variance within each class constrained to 0;

OUTPUT: TECH8 TECH4 TECH10 TECH11 SVALUES

